<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Information</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.min.js"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Google Material Icons -->

    <script src="/js/header.js" defer></script>
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@200;400&display=swap');
    </style>

    <link rel="stylesheet" href="/css/table_style.css">
    <style>
        .container {
            margin-left: 15px !important;
            margin-right: 15px !important;
        }

        .table-responsive {
            width: 95vw !important;
        }

        h2 {
            color: #000000 !important;
        }

        .mb-4 a {
            background-color: white !important;
        }

        i {
            cursor: pointer;
            color: #4d44b5 !important;
        }

        #goBackButton {
            background-color: azure !important;
            border-radius: 0px !important;
        }
    </style>

</head>

<body>
    <div class="container">
        <h2> <button id="goBackButton"><i class="material-icons">arrow_back</i></button>Student Information</h2>
        <div class="table-responsive">
            <table class="table table-light table-striped">
                <thead>
                    <tr>
                        <th scope="col">Enroll No.</th>
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">Gender</th>
                        <th scope="col">Mobile No.</th>
                        <th scope="col">Guardian's Name</th>
                        <th scope="col">Guardian's No.</th>
                        <th scope="col">CGPA</th>
                        <th scope="col">Course</th>
                        <th scope="col">Programme</th>
                        <th scope="col">TG ID</th>
                    </tr>
                </thead>
                <tbody>
                    <% Data.forEach(Student=> { %>

                        <tr>
                            <th scope="row">
                                <%=Student.enrollment %>
                            </th>
                            <td>
                                <%=Student.name %>
                            </td>
                            <td>
                                <%=Student.email %>
                            </td>
                            <td>
                                <%=Student.gender%>
                            </td>
                            <td>
                                <%=Student.mobile %>
                            </td>

                            <td>
                                <%=Student.guardianName%>
                            </td>
                            <td>
                                <%=Student.guardianMobile%>
                            </td>
                            <td>
                                <%=Student.cgpa %>
                            </td>
                            <td>
                                <%=Student.branch%>
                            </td>
                            <td>
                                <%=Student.programme%>
                            </td>
                            <td>
                                <%=Student.tgid%>
                            </td>

                        </tr>
                        <%}); %>

                </tbody>
            </table>
        </div>
    </div>
    <script>
        // Function to go back to the previous page
        function goBack() {
            window.history.back();
        }

        // Add click event listener to the button
        const backButton = document.getElementById("goBackButton");
        backButton.addEventListener("click", goBack);
    </script>
</body>

</html>